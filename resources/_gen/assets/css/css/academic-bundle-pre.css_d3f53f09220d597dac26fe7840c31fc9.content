h1,h2,h3,h4,h5,h6{font-weight:400}h1{font-size:2.25rem}h2{margin-top:2rem;font-size:1.4rem;font-weight:600}h3{font-weight:600;margin-top:1.5rem;font-size:1.2rem}h4,h5,h6{font-weight:600;margin-top:1rem;font-size:1rem}code{font-size:1rem}pre{font-size:.9rem}.article-categories{padding:0 .3em 0 .4em;margin:.1em 0;font-weight:100;font-size:.7rem;color:#fff;background:#4052b5;border:solid 1px #4052b5;border-radius:3px;display:inline-block}.article-categories a{color:#fff}.article-categories a:hover{color:#fff}.article-tags{padding:0 .3em 0 .4em;margin:.1em 0;font-size:.7rem;font-weight:100;color:#4052b5;background:#fff;border:solid 1px #4052b5;border-radius:3px;display:inline-block}.article-tags a{color:#4052b5}.article-tags a:hover{color:#4052b5}video{width:100%;height:auto}.media.stream-item .stream-meta{margin-top:6px}.media.stream-item img{max-width:250px;max-height:200px;object-fit:cover}.article-date{display block;float:right;border-bottom:1px solid #4052b5;font-weight:200;font-style:italic;font-size:.7rem;color:#4052b5}.media.stream-item .article-title,.card-simple .article-title{font-size:1.1rem}.embed-ogimage{align-content:center;text-align:center}.embed-ogimage img{width:800px;height:auto;object-fit:cover}